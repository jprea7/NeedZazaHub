         while _G.autoShoot do
            local args = {
               [1] = 1
           }
           
           game:GetService("ReplicatedStorage"):FindFirstChild("events-O67"):FindFirstChild("a29a579c-066f-4787-a2f5-b66e701e925e"):FireServer(unpack(args))
            task.wait(3)
         end
      end,
   })

